{
    "name": "wc3maptranslator",
    "version": "4.0.4",
    "description": "Module to translate between `war3map` and `json` formats for WarCraft III .w3x maps",
    "keywords": [
        "wc3",
        "warcraft",
        "reforged",
        "frozen throne",
        "reign of chaos",
        "war3map",
        "translator",
        "json",
        "w3x"
    ],
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "files": [
        "dist/src/**"
    ],
    "scripts": {
        "build": "tsc --build",
        "rebuild": "npm run clean && npm run build",
        "clean": "npx rimraf dist",
        "lint": "tslint -c tslint.json '{src,test}/**/*.ts'",
        "test": "npm run rebuild && mocha ./dist/test/**/*.js",
        "test-nyc": "npm run build && nyc mocha",
        "test-travis": "npm run build && ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --include-all-sources true --root ./lib -- -R spec ./test/*.js"
    },
    "engines": {
        "node": ">=22",
        "npm": ">=11",
        "tsc": ">4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://chiefofgxbxl@github.com/ChiefOfGxBxL/WC3MapTranslator.git"
    },
    "author": "ChiefOfGxBxL",
    "contributors": [
        "ChiefOfGxBxL",
        "dulingzhi",
        "rufreakde",
        "ttay24",
        "yatyricky",
        "Slayer95"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ChiefOfGxBxL/WC3MapTranslator/issues"
    },
    "homepage": "https://github.com/ChiefOfGxBxL/WC3MapTranslator#readme",
    "dependencies": {
        "ieee754": "^1.2.1",
        "intn": "^1.0.0",
        "round-to": "^5.0.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/fs-extra": "^9.0.7",
        "@types/mocha": "^8.2.1",
        "@types/node": "^14.14.31",
        "@types/round-to": "^4.0.0",
        "coveralls": "^3.1.0",
        "fs-extra": "^9.1.0",
        "istanbul": "^0.4.5",
        "mocha": "^8.3.0",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "typescript": "^4.2.2"
    }
}
